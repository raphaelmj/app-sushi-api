{
  "name": "sushi-orders",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "random:decimal": "ts-node -r tsconfig-paths/register src/console.ts random-decimal-price",
    "users": "ts-node -r tsconfig-paths/register src/console.ts create-users",
    "orders": "ts-node -r tsconfig-paths/register src/console.ts orders-status",
    "options": "ts-node -r tsconfig-paths/register src/console.ts create-options",
    "import:menu": "ts-node -r tsconfig-paths/register src/console.ts import-menu",
    "create:config": "ts-node -r tsconfig-paths/register src/console.ts create-config",
    "create:config:bonus": "ts-node -r tsconfig-paths/register src/console.ts config-add-bonus",
    "create:config:timezone": "ts-node -r tsconfig-paths/register src/console.ts config-timezone-lang",
    "set:orders:extra:price": "ts-node -r tsconfig-paths/register src/console.ts extra-price-one-set",
    "set:bonus:price": "ts-node -r tsconfig-paths/register src/console.ts bonus-prices-set",
    "refactor:manynames": "ts-node -r tsconfig-paths/register src/console.ts change-many-names",
    "refactor:desc:sea": "ts-node -r tsconfig-paths/register src/console.ts change-desc-sea",
    "refactor:elements:gluten:grill": "ts-node -r tsconfig-paths/register src/console.ts set-grill-and-gluten",
    "refactor:elements:options": "ts-node -r tsconfig-paths/register src/console.ts create-options-strings",
    "refactor:carts:elements": "ts-node -r tsconfig-paths/register src/console.ts set-ind-config",
    "refactor:carts:elements:plus": "ts-node -r tsconfig-paths/register src/console.ts set-ind-config-plus",
    "refactor:menu:elements:config": "ts-node -r tsconfig-paths/register src/console.ts set-config-step-if-empty",
    "refactor:elements:config:many": "ts-node -r tsconfig-paths/register src/console.ts set-price-to-config-step-many",
    "refactor:elements:config:many:plus": "ts-node -r tsconfig-paths/register src/console.ts set-price-to-config-step-many-plus",
    "recount:price:config:many": "ts-node -r tsconfig-paths/register src/console.ts recount-price-config-step-many",
    "recount:order:total": "ts-node -r tsconfig-paths/register src/console.ts recount-total",
    "create:index": "ts-node -r tsconfig-paths/register src/console.ts create-index",
    "update:index": "ts-node -r tsconfig-paths/register src/console.ts update-index",
    "create:menuids": "ts-node -r tsconfig-paths/register src/console.ts set-menuelement-id",
    "all:create": "npm run users && npm run options && npm run import:menu && npm run create:config"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.0",
    "@nestjs/common": "^7.4.1",
    "@nestjs/core": "^7.4.1",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.1",
    "@nestjs/platform-socket.io": "^7.4.1",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.4.1",
    "@types/exceljs": "^1.3.0",
    "@types/faker": "^4.1.12",
    "@types/helmet": "0.0.47",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "@types/slug": "^0.9.1",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.0.0",
    "commander": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "exceljs": "^4.1.1",
    "faker": "^4.1.0",
    "helmet": "^4.0.0",
    "moment": "^2.27.0",
    "moment-precise-range-plugin": "^1.3.0",
    "mongoose": "^5.9.26",
    "mysql": "^2.18.1",
    "ntp-time-sync": "^0.1.0",
    "p-iteration": "^1.1.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "slug": "^3.3.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.4.1",
    "@types/express": "^4.17.7",
    "@types/jest": "26.0.7",
    "@types/mongoose": "^5.7.34",
    "@types/node": "^14.0.27",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.2.1",
    "nestjs-console": "^3.1.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.4",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}